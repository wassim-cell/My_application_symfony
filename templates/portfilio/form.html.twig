
<div id="form" style="width:50%;border:2px black;margin-left:25%;margin-top:5%;
border: 2px solid black;padding:20px;" class="col-md-4 col-md-offset-4">
{{ form_start(form) }}
  <div style="text-align:center;"><h2>Full the form</h2></div>
  <div class="form-group">
    <label for="exampleInputEmail1">name :</label>
    {{ form_widget(form.name ) }}
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">message :</label>
    {{ form_widget(form.message ) }}
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Discription :</label>
    {{ form_widget(form.discription ) }}
  </div> <br>
  {{ form_widget(form.imageFile ) }}
  <div style="padding-left:35%;">
  {{ form_widget(form.save ) }}
  </div>
  {{ form_end(form) }}
</div>
